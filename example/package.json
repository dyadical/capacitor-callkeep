{
  "scripts": {
    "sync": "cap sync android",
    "start": "webpack watch --mode=development ./src/main.js -o ./www/build/",
    "build": "webpack build --mode=development ./src/main.js -o ./www/build/ && npx cap sync android",
    "buildprod": "webpack build --mode=production ./src/main.js -o ./www/build/ && npx cap sync android",
    "fmt": "npx prettier --write src/main.js www/index.html README.md"
  },
  "type": "module",
  "dependencies": {
    "@capacitor/android": "^3.2.2",
    "@capacitor/core": "^3.2.2",
    "@capacitor/push-notifications": "^1.0.4",
    "capacitor-callkeep": "file:../"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.2.2",
    "webpack-cli": "^4.8.0"
  }
}
